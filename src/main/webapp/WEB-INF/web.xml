<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>dental-mechanic</display-name>

<!--  <listener>-->
<!--    <listener-class>edu.dental.web.servlets.basic.ContextInitializer</listener-class>-->
<!--  </listener>-->

  <servlet>
    <servlet-name>EnterServlet</servlet-name>
    <servlet-class>edu.dental.web.servlets.basic.EnterServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Prime</servlet-name>
    <servlet-class>edu.dental.web.servlets.basic.Prime</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ProductTypePage</servlet-name>
    <servlet-class>edu.dental.web.servlets.works.ProductTypePage</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EnterServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Prime</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ProductTypePage</servlet-name>
    <url-pattern>/new-product-type</url-pattern>
  </servlet-mapping>

</web-app>
