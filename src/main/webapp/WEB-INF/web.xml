<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>dental-mechanic</display-name>

  <servlet>
    <servlet-name>ServerApp</servlet-name>
    <servlet-class>edu.dental.server.application.ServerApp</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ServerWelcome</servlet-name>
    <servlet-class>edu.dental.server.application.ServerWelcome</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServerApp</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServerWelcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>

</web-app>
